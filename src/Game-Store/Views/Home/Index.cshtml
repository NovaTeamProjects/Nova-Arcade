@using Game_Store.Domain.Entities

@model List<Game>
<style>
    .content {
        text-align: center;
    }

    .container {
        margin-left: 100px !important;
    }

    .card img {
        width: 200px;
    }

    .card-body2 {
        padding-top: 10px;
        display: flex;
        gap: 10px;
    }

    .discount {
        background-color: #0074e5;
        border-radius: 4px;
        padding-top: 3px;
        padding-left: 3px;
        padding-right: 3px;
        padding-bottom: 3px;
    }
</style>
@foreach(var game in Model)
{
    <div class="container">
        <div class="card" style="width: 18rem;">
            <img src="data:image/jpeg;base64," class="card-img-top" alt="">
            <div class="card-body">
                <p class="font-monospace" style="font-size:14px;">@game.Name</p>
                <h5 class="card-title">@game.Name</h5>
            </div>
            <div class="card-body card-body2">
                <p class="discount">@game.Price</p>
                <p style="text-decoration:line-through;">@game.Price</p>
                <p>@game.Price</p>
            </div>
        </div>
    </div>
}